mixin tableRow(set)
    tr
        td
            if (set.parent == null)
                i(class='ss ss-'+set.code+' ss-2x')
            else
                i.ellipsis.vertical.icon
                i(class='ss ss-'+set.code+' ss-2x')
        td #{set.name} (#{set.code})
        td #{set.card_count}
        td #{set.release_date}
        td
            // EN
            if (set.languages.some(e => e.code == 'en'))
                a(href='/set/' + set.code + '/en?web=true')
                    button.mini.ui.positive.button EN
            else
                button.mini.ui.disabled.button EN

            // ES
            if (set.languages.some(e => e.code == 'es'))
                a(href='/set/' + set.code + '/es?web=true')
                    button.mini.ui.positive.button ES
            else
                button.mini.ui.disabled.button ES

            // FR
            if (set.languages.some(e => e.code == 'fr'))
                a(href='/set/' + set.code + '/fr?web=true')
                    button.mini.ui.positive.button FR
            else
                button.mini.ui.disabled.button FR

            // DE
            if (set.languages.some(e => e.code == 'de'))
                a(href='/set/' + set.code + '/de?web=true')
                    button.mini.ui.positive.button DE
            else
                button.mini.ui.disabled.button DE

            // IT
            if (set.languages.some(e => e.code == 'it'))
                a(href='/set/' + set.code + '/it?web=true')
                    button.mini.ui.positive.button IT
            else
                button.mini.ui.disabled.button IT

            // PT
            if (set.languages.some(e => e.code == 'PT'))
                a(href='/set/' + set.code + '/pt?web=true')
                    button.mini.ui.positive.button PT
            else
                button.mini.ui.disabled.button PT

            // JA
            if (set.languages.some(e => e.code == 'ja'))
                a(href='/set/' + set.code + '/ja?web=true')
